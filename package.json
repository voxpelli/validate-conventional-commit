{
  "name": "validate-conventional-commit",
  "version": "1.0.2",
  "description": "Smallest simplest conventional commit validator to use with eg Husky",
  "homepage": "http://github.com/voxpelli/validate-conventional-commit",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/validate-conventional-commit.git"
  },
  "keywords": [
    "husky",
    "conventional-commit",
    "commitlint",
    "commit",
    "git",
    "commit-msg"
  ],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "0BSD",
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "bin": {
    "linemod": "cli.js"
  },
  "files": [
    "cli.js"
  ],
  "scripts": {
    "check:lint": "eslint --report-unused-disable-directives .",
    "check:tsc": "tsc",
    "check": "run-p check:*",
    "prepare": "husky install",
    "test": "run-s check"
  },
  "devDependencies": {
    "@types/node": "^16.18.32",
    "@voxpelli/eslint-config": "^17.0.3",
    "@voxpelli/tsconfig": "^7.0.0",
    "eslint": "^8.43.0",
    "eslint-plugin-es-x": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^44.2.7",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "husky": "^8.0.3",
    "npm-run-all2": "^6.0.5",
    "typescript": "~5.0.4"
  }
}
